<display>
    <table length="20" time-interval-rule="lifetime_decreased_monthly" csvFileName="signup_analysis.csv">
        <date section="Signup Analysis" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" formatLifetime="MMM yyyy"/>
        <metric type="total_users" title="Total Users"/>
        <metric type="user_created" title="Total Accounts (Logged)"/>
        <metric type="users_created_project_once" title="Created Project"/>
        <metric type="users_built_once" title="&amp; Built"/>
        <metric type="users_deployed_once" title="&amp; Deployed"/>
        <metric type="users_deployed_paas_once" title="&amp; Deployed to PAAS"/>
