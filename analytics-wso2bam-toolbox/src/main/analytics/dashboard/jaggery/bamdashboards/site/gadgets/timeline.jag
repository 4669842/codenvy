<%

include_once("utils.jag");

var timeGrouping = request.getParameter('timeGrouping');
var tableNames = ['workspaces', 'workspaces_usage', 'users', 'users_usage', 'users_engagement'];

for (var i = 0; i < tableNames.length; i++) {
    var result = queryResults('select * from ' + tableNames[i] + '_' + timeGrouping);
    var table = createTable(result);

    printTable(table);
}

