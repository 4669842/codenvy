            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="project_paas_cloudbees"/>
                <parameter key="description" value="CloudBees"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="project_paas_cloudfoundry"/>
