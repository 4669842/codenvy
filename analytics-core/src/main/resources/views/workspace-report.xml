<display>
    <view time-unit="day,week,month,lifetime" name="workspace_report" columns="15">
        <section name="workspaces">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Workspaces"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="created_workspaces"/>
                <parameter key="description" value="Created"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="destroyed_workspaces"/>
                <parameter key="description" value="Destroyed"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="total_workspaces"/>
                <parameter key="description" value="Total"/>
            </row>
        </section>

        <section name="workspace_usage">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Workspace Usage"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="total_workspaces"/>
                <parameter key="description" value="Total"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_active_workspaces"/>
                <parameter key="description" value="New Active Workspaces"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="returning_active_workspaces"/>
                <parameter key="description" value="Returning Active Workspaces"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="non_active_workspaces"/>
                <parameter key="description" value="Non-Active Workspaces"/>
            </row>
        </section>
    </view>
</display>