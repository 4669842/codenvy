<display>
    <view time-unit="day,week,month,lifetime" name="sessions_report" columns="15">
        <section name="user_sessions">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="# User Sessions"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_total"/>
                <parameter key="description" value="Total"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_below_1_min"/>
                <parameter key="description" value="&#0060;= 1 Min"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_between_1_and_10_min"/>
                <parameter key="description" value="&#0062; 1 And &#0060; 10 Mins"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_between_10_and_60_min"/>
                <parameter key="description" value="&#0062;= 10 And &#0060;= 60 Mins"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_above_60_min"/>
                <parameter key="description" value="&#0062; 60 Mins"/>
            </row>
            <row class="com.codenvy.analytics.services.view.EmptyRow"/>
        </section>
        
        <section name="sessions_splits">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Sessions Splits"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions_total"/>
                <parameter key="description" value="Total"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="factory_sessions"/>
                <parameter key="description" value="Factory Sessions"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="product_usage_sessions"/>
                <parameter key="description" value="User Sessions"/>
            </row>            
            <row class="com.codenvy.analytics.services.view.EmptyRow"/>
        </section>
    </view>
</display>