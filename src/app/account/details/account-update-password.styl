.change-password .prompt
  width 14%
  display inline-block
  color $disabled-color

.change-password div[ng-show],
.change-password div[ng-message]
  position absolute

.change-password .che-input-desktop
  margin-bottom 0

.change-password .prompt + .che-input .che-input-desktop
  margin-top 30px
  margin-bottom 50px

