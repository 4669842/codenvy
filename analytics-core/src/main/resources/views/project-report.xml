            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="project_paas_openshift"/>
                <parameter key="description" value="OpenShift"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
