.factory-commands-panel
  margin-top -6px

  .factory-commands-input
    min-height 50px
    height 50px

  .che-input
    margin-right 20px

  .factory-commands-list
    max-width 600px

    div
      outline none

    md-icon
      font-size 18px
      height auto
      width auto
      color $clear-foggy-sky-color
      line-height 18px

    .factory-commands-widget-actionicons
      outline none
      cursor pointer
      margin-left 10px

    .factory-commands-row-command-name
      min-width 220px
      width 220px

      span
        padding 2px

    .factory-commands-row-command
      cursor pointer

    .factory-commands-row-actions
      height 21px
      min-width 60px
