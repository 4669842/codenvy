<display>
    <view time-unit="day,week,month,lifetime" name="tasker_report" columns="15">
        <section name="summary">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Summary"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_runs_memory_usage_per_hour"/>
                <parameter key="description" value="Gigabyte RAM Hours"/>
                <parameter key="numeric-format" value="%.1f"/>
            </row>
            <!--  TODO DASHB-598 -->
        </section>

        <section name="builder">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Builder Info"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds"/>
                <parameter key="description" value="Builders Launched"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_finished"/>
                <parameter key="description" value="Builders Stopped"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_time"/>
                <parameter key="description" value="Builder Time"/>
                <parameter key="time-field" value="true"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_memory_usage_per_hour"/>
                <parameter key="description" value="Gigabyte RAM Hours"/>
                <parameter key="numeric-format" value="%.1f"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_with_timeout"/>
                <parameter key="description" value="Builders Launched w/ Timeout"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_with_always_on"/>
                <parameter key="description" value="Builders Launched w/ Always-On"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_finished_normally"/>
                <parameter key="description" value="Builders Stopped Normally"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_finished_by_timeout"/>
                <parameter key="description" value="Builders Stopped by Timeout"/>
            </row>
        </section>

        <section name="runner">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Runner Info"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs"/>
                <parameter key="description" value="Runners Launched"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_finished"/>
                <parameter key="description" value="Runners Stopped"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_time"/>
                <parameter key="description" value="Runners Time"/>
                <parameter key="time-field" value="true"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_memory_usage_per_hour"/>
                <parameter key="description" value="Gigabyte RAM Hours"/>
                <parameter key="numeric-format" value="%.1f"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_with_timeout"/>
                <parameter key="description" value="Runners Launched w/ Timeout"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_with_always_on"/>
                <parameter key="description" value="Runners Launched w/ Always-On"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_finished_by_user"/>
                <parameter key="description" value="Runners Stopped by User"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_finished_by_timeout"/>
                <parameter key="description" value="Runners Stopped by Timeout"/>
            </row>
        </section>

    </view>
</display>
