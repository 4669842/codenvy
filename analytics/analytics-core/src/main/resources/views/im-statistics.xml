<display>
    <view name="im_downloads_list" on-demand="true">
        <section name="im_downloads">
            <row class="com.codenvy.analytics.services.view.TitleRow">
                <parameter key="titles" value="Date,User,Artifact,Version"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_downloads_list"/>
                <parameter key="fields" value="date,user,artifact,version"/>
                <parameter key="date-fields" value="date"/>
            </row>
        </section>
    </view>

    <view name="im_on_prem_issued_list" on-demand="true">
        <section name="im_on_prem_issued">
            <row class="com.codenvy.analytics.services.view.TitleRow">
                <parameter key="titles" value="Date,User,Plan"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_on_prem_issued_list"/>
                <parameter key="fields" value="date,user,plan"/>
                <parameter key="date-fields" value="date"/>
            </row>
        </section>
    </view>

    <view time-unit="day,week,month,lifetime" name="im_analysis" columns="15">
        <section name="im_downloads_summary">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value=""/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_downloads_install_script"/>
                <parameter key="description" value="Number of on-prem install script executions"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_downloads_codenvy"/>
                <parameter key="description" value="Number of on-prem Codenvy downloads"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_on_prem_issued"/>
                <parameter key="description" value="Number of on-prem trial licenses issued"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="cdec_first_login"/>
                <parameter key="description" value="Number of first logins"/>
            </row>
        </section>
        <section name="im_analysis">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value=""/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="total_users"/>
                <parameter key="description" value="Total Users"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="created_users"/>
                <parameter key="description" value="Users - Added"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_usage_time_greater_1_min"/>
                <parameter key="description" value="New Users - # with &#0062;1min Usage"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_usage_time_greater_60_min"/>
                <parameter key="description" value="New Users - # with &#0062;60min Usage"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="created_projects"/>
                <parameter key="description" value="New Projects"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_gb_hours"/>
                <parameter key="description" value="New Users GB Hrs"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_builds"/>
                <parameter key="description" value="New Users Builds"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_runs"/>
                <parameter key="description" value="New Users Runs"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_deploys"/>
                <parameter key="description" value="New Users Deploys"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_sent_invites"/>
                <parameter key="description" value="New Users Sent invites"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_on_prem_downloads"/>
                <parameter key="description" value="New Users Codenvy Downloads"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="new_users_on_prem_trial_licenses"/>
                <parameter key="description" value="New Users Trial Licenses"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_installs_started"/>
                <parameter key="description" value="Number of on-prem Codenvy installs started"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_installs_finished_successfully"/>
                <parameter key="description" value="Number of on-prem Codenvy installs finished successfully"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="im_installs_finished_unsuccessfully"/>
                <parameter key="description" value="Number of on-prem Codenvy installs finished unsuccessfully"/>
            </row>
        </section>
    </view>
</display>
