<views history-length="20">
    <view>
        <date section="Workspaces" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="workspaces_created_number" title="Created"/>
        <metric type="workspaces_destroyed_number" title="Destroyed"/>
        <metric type="total_workspaces_number" title="Total"/>
        <metric type="active_workspaces_number" title="# Active"/>
        <metric type="active_workspaces_percent" format="%.1f%%" title="% Active"/>
        <empty />
    </view>
    <view>
        <date section="Users" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
		<metric type="users_created_number" title="Created"/>
        <metric type="users_created_projects_percent" format="%.0f%%" title="% Have Created Projects"/>
		<metric type="users_destroyed_number" title="Purged"/>
        <metric type="total_users_number" title="Total"/>
        <metric type="active_users_number" title="# Active"/>
		<metric type="active_users_percent" format="%.1f%%" title="% Active"/>
        <empty />
	</view>
	<view>		
		<date section="Added To Workspace" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="users_added_to_workspace_from_website_percent" format="%.0f%%" title="From Website"/>
        <metric type="users_added_to_workspace_from_invite_percent" format="%.0f%%" title="By Invitation"/>
        <empty />
	</view>
	<view>
		<date section="Users Logged in Ways" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="users_sso_logged_in_using_google_percent" format="%.0f%%" title="Google Auth"/>
        <metric type="users_sso_logged_in_using_github_percent" format="%.0f%%" title="Github Auth"/>
        <metric type="users_sso_logged_in_using_form_percent" format="%.0f%%" title="Form Auth"/>
        <empty />
    </view>
    <view>
        <date section="Sessions" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="product_usage_time_total" format="%,.0f" title="Product usage time (mins)"/>
        <metric type="product_usage_user_sessions_number" format="%,.0f" title="# User Sessions"/>
        <empty />
    </view>
    <view>
        <date section="Projects" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="projects_created_number" title="Created"/>
        <metric type="built_projects_percent" format="%.0f%%" title="% Built"/>
		<metric type="projects_destroyed_number" title="Destroyed"/>
        <metric type="total_projects_number" title="Total"/>
        <metric type="active_projects_number" title="# Active"/>
        <metric type="active_projects_percent" format="%.1f%%" title="% Active"/>
        <empty />
    </view>
    <view>
        <date section="# Project Types" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="project_type_java_jar_number" format="%.0f" title="Java Jar"/>
        <metric type="project_type_java_war_number" format="%.0f" title="Java War"/>
        <metric type="project_type_java_jsp_number" format="%.0f" title="Java JSP"/>
        <metric type="project_type_java_spring_number" format="%.0f" title="Java Spring"/>
        <metric type="project_type_php_number" format="%.0f" title="PHP"/>
        <metric type="project_type_python_number" format="%.0f" title="Python"/>
        <metric type="project_type_javascript_number" format="%.0f" title="JavaScript"/>
        <metric type="project_type_ruby_number" format="%.0f" title="Ruby"/>
        <metric type="project_type_mmp_number" format="%.0f" title="Maven Multi Project"/>
        <metric type="project_type_nodejs_number" format="%.0f" title="Node.js"/>
        <metric type="project_type_others_number" format="%.0f" title="Others"/>
        <empty />
    </view>
    <view>
        <date section="% Project Types" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="project_type_java_jar_percent" format="%.1f%%" title="Java Jar"/>
        <metric type="project_type_java_war_percent" format="%.1f%%" title="Java War"/>
        <metric type="project_type_java_jsp_percent" format="%.1f%%" title="Java JSP"/>
        <metric type="project_type_java_spring_percent" format="%.1f%%" title="Java Spring"/>
        <metric type="project_type_php_percent" format="%.1f%%" title="PHP"/>
        <metric type="project_type_python_percent" format="%.1f%%" title="Python"/>
        <metric type="project_type_javascript_percent" format="%.1f%%" title="JavaScript"/>
        <metric type="project_type_ruby_percent" format="%.1f%%" title="Ruby"/>
        <metric type="project_type_mmp_percent" format="%.1f%%" title="Maven Multi Project"/>
        <metric type="project_type_nodejs_percent" format="%.1f%%" title="Node.js"/>
        <metric type="project_type_others_percent" format="%.1f%%" title="Others"/>
        <empty />
    </view>
    <view>
        <date section="JRebel Usage" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="jrebel_eligible" title="Eligible"/>
        <metric type="jrebel_usage_percent" format="%.0f%%" title="% Usage"/>
        <empty />
    </view>
    <view>
        <date section="# Deployments" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="paas_deployment_type_aws_number" format="%.0f" title="AWS"/>
        <metric type="paas_deployment_type_appfog_number" format="%.0f" title="AppFog"/>
        <metric type="paas_deployment_type_cloudbees_number" format="%.0f" title="CloudBees"/>
        <metric type="paas_deployment_type_cloudfoundry_number" format="%.0f" title="CloudFoundry"/>
        <metric type="paas_deployment_type_gae_number" format="%.0f" title="GAE"/>
        <metric type="paas_deployment_type_heroku_number" format="%.0f" title="Heroku"/>
        <metric type="paas_deployment_type_openshift_number" format="%.0f" title="OpenShift"/>
        <metric type="paas_deployment_type_tier3_number" format="%.0f" title="Tier3"/>
        <metric type="paas_deployment_type_local_number" format="%.0f" title="Local"/>
        <empty />
    </view>
    <view>
        <date section="% Deployments" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="paas_deployment_type_aws_percent" format="%.1f%%" title="AWS"/>
        <metric type="paas_deployment_type_appfog_percent" format="%.1f%%" title="AppFog"/>
        <metric type="paas_deployment_type_cloudbees_percent" format="%.1f%%" title="CloudBees"/>
        <metric type="paas_deployment_type_cloudfoundry_percent" format="%.1f%%" title="CloudFoundry"/>
        <metric type="paas_deployment_type_gae_percent" format="%.1f%%" title="GAE"/>
        <metric type="paas_deployment_type_heroku_percent" format="%.1f%%" title="Heroku"/>
        <metric type="paas_deployment_type_openshift_percent" format="%.1f%%" title="OpenShift"/>
        <metric type="paas_deployment_type_tier3_percent" format="%.1f%%" title="Tier3"/>
        <metric type="paas_deployment_type_local_percent" format="%.1f%%" title="Local"/>
        <empty />
    </view>
    <view>
        <date section="Invitations" formatDay="dd MMM" formatWeek="dd MMM" formatMonth="MMM yyyy" />
        <metric type="invitations_sent" title="Sent"/>
        <metric type="invitations_accepted_percent" format="%.0f%%" title="Accepted"/>
        <empty />
    </view>
</views>

