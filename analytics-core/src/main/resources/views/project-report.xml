<display>
    <view time-unit="day,week,month,lifetime" name="project_report" columns="15">
        <section name="projects">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Projects"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="created_projects"/>
                <parameter key="description" value="Created"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="destroyed_projects"/>
                <parameter key="description" value="Destroyed"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="total_projects"/>
                <parameter key="description" value="Total"/>
            </row>
        </section>

        <section name="ide_usage">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Project Event Counts"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="code_refactorings"/>
                <parameter key="description" value="# Refactors"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="code_completions"/>
                <parameter key="description" value="# Code Completions"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds"/>
                <parameter key="description" value="# Builds"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs"/>
                <parameter key="description" value="# Runs"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="debugs"/>
                <parameter key="description" value="# Debugs"/>
            </row>
        </section>

        <section name="time_spent">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Project Time"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="builds_time"/>
                <parameter key="description" value="Builds"/>
                <parameter key="time-field" value="true"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="runs_time"/>
                <parameter key="description" value="Runs"/>
                <parameter key="time-field" value="true"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="debugs_time"/>
                <parameter key="description" value="Debugs"/>
                <parameter key="time-field" value="true"/>
            </row>
        </section>

        <section name="projects_types">
            <row class="com.codenvy.analytics.services.view.DateRow">
                <parameter key="section-name" value="Project Types"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="created_projects"/>
                <parameter key="description" value="Total Created Projects in Period"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="project_type_jar"/>
                <parameter key="description" value="Java Jar"/>
            </row>
            <row class="com.codenvy.analytics.services.view.MetricRow">
                <parameter key="name" value="project_type_war"/>
